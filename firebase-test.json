{
  "hosting": {
    "target": "test",
    "public": "dist/phaenonet",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "!/@(assets)/**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/node_modules/**"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
